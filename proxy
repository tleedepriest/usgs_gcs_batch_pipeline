server {
        server_name SERVER-NAME;
        root /var/www/;

        index index.html;
        location / {
                proxy_pass http://10.128.0.104:8080;
        }

        location /flower/ {
                proxy_pass http://10.128.0.104:5555/flower/;
        }

        location = /robots.txt { return 200 "User-agent: *\nDisallow: /\n"; }
